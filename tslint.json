{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
       "eofline": false,
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "indent": false,
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            150
        ],
        "member-ordering": [
            false
        ],
        "curly": false,
        "interface-name": [
            false
        ],
        "no-empty-interface": false,
        "no-empty": false,
        "arrow-parens": false,
        "one-variable-per-declaration": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "semicolon": [
            false
        ],
        "no-console": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ]
    },
    "rulesDirectory": []
}
